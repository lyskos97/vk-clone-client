{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": ["jsx-a11y", "react", "import"],
  "rules": {
    "import/prefer-default-export": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-filename-extension": 0,
    "arrow-body-style": 0,
    "indent": 0
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
